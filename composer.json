{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "authorizenet/authorizenet": "2.0.2",
        "aws/aws-sdk-php": "*",
        "cinetpay/cinetpay-php": "^1.9",
        "doctrine/dbal": "^3.6",
        "flutterwavedev/flutterwave-v3": "^1.0",
        "google/apiclient": "^2.15",
        "google/cloud-translate": "^1.14",
        "guzzlehttp/guzzle": "^7.7",
        "hisorange/browser-detect": "^4.4",
        "infobip-community/infobip-api-php-sdk": "^1.1",
        "infobip/infobip-api-php-client": "5.0.0",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.15",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "mailersend/laravel-driver": "^2.2",
        "matthiasmullie/minify": "^1.3",
        "midtrans/midtrans-php": "^2.5",
        "mollie/laravel-mollie": "^2.14",
        "plivo/plivo-php": "^4.52",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "stevebauman/purify": "^6.0",
        "symfony/http-client": "^6.3",
        "symfony/mailchimp-mailer": "^6.3",
        "symfony/mailer": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "symfony/postmark-mailer": "^6.3",
        "symfony/sendgrid-mailer": "^6.3",
        "symfony/sendinblue-mailer": "^6.3",
        "twilio/sdk": "^7.7",
        "vonage/client": "^4.0",
        "yajra/laravel-datatables": "10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
